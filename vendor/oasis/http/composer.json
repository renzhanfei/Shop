{
    "name"             : "oasis/http",
    "description"      : "An extension to Silex, for HTTP related routing, middleware, and so on.",
    "minimum-stability": "stable",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Minhao Zhang",
            "email": "minhao.zhang@qq.com"
        }
    ],
    "require"          : {
        "symfony/http-foundation"    : "^3.0",
        "silex/silex"                : "^2.0",
        "symfony/routing"            : "^3.0",
        "symfony/config"             : "^3.0",
        "symfony/yaml"               : "^3.0",
        "symfony/expression-language": "^3.0",
        "oasis/logging"              : "^1.1",
        "oasis/utils"                : "^1.6",
        "symfony/security"           : "^3.0",
        "twig/twig"                  : "^1.24",
        "symfony/twig-bridge"        : "^3.0",
        "twig/extensions"            : "^1.3",
        "silex/providers"            : "^2.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev"      : {
        "phpunit/phpunit"     : "^5.2",
        "symfony/browser-kit" : "^3.0",
        "symfony/css-selector": "^3.0"
    },
    "autoload"         : {
        "psr-4": {
            "Oasis\\Mlib\\Http\\": "src/"
        }
    },
    "autoload-dev"     : {
        "psr-4": {
            "Oasis\\Mlib\\Http\\Test\\": "ut/"
        }
    }
}
